{
  "name": "ahc-analytics-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "per-env",
    "start:development": "vite dev --host 0.0.0.0 --strictPort --port ${HMR_PORT:-4173}",
    "start:production": "echo 'starting'",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --host 0.0.0.0 --strictPort --port ${HMR_PORT:-4173}",
    "test:unit": "vitest",
    "prepare": "per-env",
    "prepare:development": "cypress install",
    "prepare:production": "npm run build-only",
    "test:e2e": "start-server-and-test preview http://localhost:${HMR_PORT:-4173} 'cypress run --e2e --config baseUrl=http://localhost:${HMR_PORT:-4173}'",
    "test:e2e:dev": "start-server-and-test start http://localhost:${HMR_PORT:-4173} 'cypress open --e2e --config baseUrl=http://localhost:${HMR_PORT:-4173} --browser electron'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "ci-test": "npm run test:unit run && npm run build-only && npm run test:e2e"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@jsonforms/core": "^3.6.0",
    "@jsonforms/vue": "^3.6.0",
    "@jsonforms/vue-vuetify": "^3.6.0",
    "@vue/apollo-composable": "^4.2.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "pinia": "^3.0.3",
    "plotly.js-dist-min": "^3.0.3",
    "reveal.js": "^5.2.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue3-plotly-ts": "^1.0.4",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.9.2"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/reveal.js": "^5.2.0",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^15.1.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "per-env": "^1.0.2",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
