    "@nebular/bootstrap": "3.4.2",
    "@nebular/eva-icons": "4.1.2",
    "@nebular/moment": "^3.5.0",
    "@nebular/security": "4.1.2",
    "@nebular/theme": "4.1.2",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "angular-plotly.js": "^1.3.2",
