{
  "name": "ahc-analytics",
  "version": "3.2.1",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "start": "per-env",
    "ci-test": "npm run test",
    "conventional-changelog": "conventional-changelog",
    "pre-start": "per-env",
    "pre-start:development": "echo 'starting'",
    "pre-start:production": "npm run build:prod",
    "start:development": "ng serve --host 0.0.0.0 --port $HMR_PORT  --poll 1000 --disableHostCheck --sourceMap=false",
    "start:production": "echo 'done'",
    "build": "ng build",
    "build:prod:low-mem": "npm run build -- --prod --aot",
    "build:prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --aot",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "format": "prettier ./src/**/*.{ts,js,html,scss,css} --write",
    "format:ci": "prettier ./src/**/*.{ts,js,html,scss,css} --check",
    "lint": "ng lint",
    "lint:fix": "ng lint ahc-analytics --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\""
  },
  "dependencies": {
    "@ajsf/bootstrap4": "^0.4.0-beta.1",
    "@akveo/ng2-completer": "^9.0.1",
    "@angular-devkit/schematics": "^12.1.3",
    "@angular/animations": "^12.1.3",
    "@angular/cdk": "12.1.3",
    "@angular/common": "^12.1.3",
    "@angular/compiler": "^12.1.3",
    "@angular/core": "^12.1.3",
    "@angular/forms": "^12.1.3",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.3",
    "@angular/router": "^12.1.3",
    "@apollo/client": "^3.3.21",
    "@briebug/jest-schematic": "^3.1.0",
    "@nebular/auth": "8.0.0",
    "@nebular/bootstrap": "^8.0.0",
    "@nebular/eva-icons": "8.0.0",
    "@nebular/moment": "^8.0.0",
    "@nebular/security": "8.0.0",
    "@nebular/theme": "8.0.0",
    "@schematics/angular": "^12.1.3",
    "@sentry/angular": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "angular-plotly.js": "^4.0.0",
    "angularx-social-login": "^4.0.1",
    "apollo-angular": "^2.6.0",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "bootstrap": "^5.0.2",
    "classlist.js": "1.1.20150312",
    "core-js": "3.15.2",
