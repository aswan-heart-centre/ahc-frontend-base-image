{
  "name": "ahc-analytics",
  "version": "3.2.1",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve --host 0.0.0.0 --port 4200  --poll 1000 --disableHostCheck --sourceMap=false",
    "build": "ng build",
    "build:prod:low-mem": "npm run build -- --prod --aot",
    "build:prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --aot",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "format": "prettier ./src/**/*.{ts,js,html,scss,css} --write",
    "format:ci": "prettier ./src/**/*.{ts,js,html,scss,css} --check",
    "lint": "ng lint",
    "lint:fix": "ng lint ahc-analytics --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\""
  },
  "dependencies": {
    "@ajsf/bootstrap4": "^0.3.0-beta.3",
    "@akveo/ng2-completer": "^9.0.1",
    "@angular/animations": "^10.0.10",
    "@angular/cdk": "10.1.1",
    "@angular/common": "^10.0.10",
    "@angular/compiler": "^10.0.10",
    "@angular/core": "^10.0.10",
    "@angular/forms": "^10.0.10",
    "@angular/platform-browser": "^10.0.10",
    "@angular/platform-browser-dynamic": "^10.0.10",
    "@angular/router": "^10.0.10",
    "@briebug/jest-schematic": "^3.0.0",
    "@nebular/auth": "6.0.0",
    "@nebular/bootstrap": "^6.0.0",
    "@nebular/eva-icons": "6.0.0",
    "@nebular/moment": "^6.0.0",
    "@nebular/security": "6.0.0",
    "@nebular/theme": "6.0.0",
    "ag-grid-angular": "^24.1.0",
    "ag-grid-community": "^24.1.0",
    "angular-plotly.js": "^3.0.0",
    "angularx-social-login": "^3.2.0",
    "bootstrap": "^4.4.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "eva-icons": "^1.1.3",
    "hammerjs": "^2.0.8",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "jquery": "^3.5.1",
    "lodash-es": "^4.17.15",
    "moment": "2.23.0",
    "nebular-icons": "1.1.0",
    "node-sass": "^4.14.0",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "plotly.js": "^1.57.0",
    "reveal.js": "^3.8.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.6.2",
    "rxjs-compat": "6.3.0",
    "socicon": "3.0.5",
    "style-loader": "^1.1.3",
    "tslib": "^2.0.0",
